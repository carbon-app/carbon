{
  "name": "carbon",
  "version": 2,
  "public": true,
  "builds": [{ "src": "package.json", "use": "@now/next@canary" }],
  "routes": [
    { "src": "^/about(.*)", "dest": "/about" },
    { "src": "^/embed(.*)", "dest": "/embed" },
    {
      "src": "^/service-worker.js$",
      "continue": true,
      "headers": {
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    { "handle": "filesystem" },
    { "src": "^/(.*)", "dest": "/index.html" }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "github": {
    "autoAlias": false,
    "silent": true
  }
}
