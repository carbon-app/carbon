{
  "alias": "carbon.now.sh",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/image/[id].js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "40mb"
      }
    }
  ],
  "github": {
    "autoAlias": false,
    "silent": true
  }
}